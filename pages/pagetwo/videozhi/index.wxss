/* pages/lookgift/index.wxss */
.txt_jt {
  position: fixed;
  left: 40rpx;
  width: 12px;
  height: 12px;
  border-top: 2px solid white;
  border-right: 2px solid white;
  transform: rotate(225deg);
  z-index: 999;
}
.topbank {
  width: 100%;
  height: 450rpx;
  background-size: 100% 100%;
  position: relative;
  z-index: 1;

}

.topbank image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.title_page{
  font-size: 52rpx;
  position: absolute;
  width: 90%;
  left: 5%;
  bottom:170rpx;
  z-index: 2;
  color: white;
}
.content{
  background: white;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  margin-top: -80rpx;
  height: 900rpx;
  position: relative;
  z-index: 2;
  overflow-y: scroll;
}
.txt_delit{
  width: 90%;
  margin: 0 auto;
  font-size: 34rpx;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
}
.video_vont{
  width: 90%;
  margin: 0 auto;
  border-radius: 14rpx;
  overflow: hidden;
  height: 224px;
}
.submits{
  font-size: 30rpx;
  width: 90%;
  position: fixed;
  left:5%;
  bottom: 3%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color:white;
  background:#29C3C7;
  border-radius: 40rpx;
  z-index: 5;
}