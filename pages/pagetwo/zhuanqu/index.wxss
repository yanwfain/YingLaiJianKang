/* pages/zhuanqu/index.wxss */
.iomgyun{
  width: 100%;
}
.iomgyun image{
  width: 100%;
  height: 100%;
}
.list_bunew {
  width: 96%;
  display: flex;
  flex-wrap: wrap;
  margin: 0 auto;
  margin-top: 30rpx;
}

.list {
  width: 48%;
  margin: 2% 1%;
  box-shadow: 1px 2px 6px #E0E0E0;
  border-radius: 14rpx;
  overflow: hidden;
  padding-bottom: 20rpx;
}

.list image {
  width: 100%;
  height: 326rpx;
}

.tit_bunew {
  width: 94%;
  margin: 0 auto;
  font-size: 32rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.botmodelt {
  width: 94%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.num_new {
  color: #767676;
}

.pick {
  color: #ED6F00;
  font-weight: 600;
  font-size: 32rpx;
}

.btnsubmit{
  padding: 4rpx 14rpx;
  font-size: 24rpx;
  border-radius: 60rpx;
  border:1px solid #ED6F00;
  color: #ED6F00;
  font-weight: 600;
}