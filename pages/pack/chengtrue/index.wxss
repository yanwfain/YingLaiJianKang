/* pages/chengtrue/index.wxss */
page{
  background: #F6F6F6;
}
.top_orderstatus{
  /* width: 100%; */
  padding: 30rpx;
  background: white;
}
.status{
  font-size: 32rpx;
  color: #191919;
  padding-top: 70rpx;
  padding-bottom: 30rpx;
  text-align: center;

}
.pickers_order{
  font-size: 56rpx;
  font-weight: 600;
  color: #15A7AA;
  text-align: center;
}
.lsit{
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  background: #F6F6F6;
  border-radius: 12rpx;
  margin-top: 50rpx;
}
.lsit image{
  width: 40rpx;
  height: 40rpx;

}
.txt_oerd{
  width: 80%;
  
}
.look_btn{
  width: 200rpx;
  color: #15A7AA;
  border-radius: 60rpx;
  text-align: center;
  line-height: 64rpx;
  border:1px solid #15A7AA;
  margin: 0 auto;
  margin-top: 50rpx;
  
}
.gengduo{
  padding: 30rpx;
  text-align: center;
  margin-top: 20rpx;
  font-size: 32rpx;
}
.list_bunew {
  width: 96%;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;

}

.list {
  width: 48%;
  margin: 2% 1%;
  box-shadow: 1px 2px 6px #E0E0E0;
  border-radius: 14rpx;
  overflow: hidden;
  padding-bottom: 20rpx;
}

.list image {
  width: 100%;
  height: 326rpx;
}

.tit_bunew {
  width: 94%;
  margin: 0 auto;
  font-size: 32rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.botmodelt {
  width: 94%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.num_new {
  color: #767676;
}

.pick {
  color: #ED6F00;
  font-weight: 600;
  font-size: 32rpx;
}

