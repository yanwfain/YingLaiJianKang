page{
 
}
.flex {
  display: flex;
  align-items: center;
}

.audioPlayer {
  width: 100%;
  /* height: 400rpx; */
  /* margin-bottom: 30rpx; */
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  position: fixed;
  bottom:10%;
  left: 0;
}

.player {
  width: 100%;
  /* height: 100%; */
  position: relative;
}

.audioBack {
  width: 100%;
  height: 100%;
}

.audioControls {
  width: 100%;
  height: 80rpx;
  /* background: black; */
  opacity: 0.8;
  position: absolute;
  bottom: 0;
  color: white;
  font-size: 6pt;
  line-height: 80rpx;
  text-align: center;
}

.audioControls .bottom {
  width: 60rpx;
  height: 100%;
}

.audioControls .bottom image {
  margin-top: 30%;
  margin-left: 30%;
  width: 40rpx;
  height: 40rpx;
}

.audioControls .slider {
  width: 520rpx;
  height: 100%;
}

.slider slider {
  width: 95%;
  margin-left: 4%;
  margin-right: 0;
  color: gray;
  /* color:rgb(223, 127, 40);
  background: rgb(223, 127, 40); */
}

.audioControls .time {
  width: 120rpx;
  height: 100%;
  font-size: 28rpx;
  color: white;
}

.start{
  width: 100%;
  position: fixed;
  bottom: 40%;
  left: 0;
}
.start .lo1{
  width:300rpx;
  height: 300rpx;
  margin: 0 auto;
}
.start view image{
  width: 100%;
  height: 100%;
}
.ltitle{
  display: flex;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
}
.titless{
  margin-top: 20rpx;
  margin-bottom: 100rpx;
  font-size: 46rpx;
  font-weight: 600;
  text-align: center;
  color: white;
}