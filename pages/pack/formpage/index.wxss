/* pages/formpage/index.wxss */
.top_view{
  width: 94%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
}
.time{
  font-size:26rpx;
  color: #767676;
}
.btnsub{
  border:1px solid #29C3C7;
  padding: 10rpx 26rpx;
  background: #F5FFFF;
  color: #29C3C7;
  border-radius: 60rpx;
}
.title{
  width: 94%;
  padding:20rpx 0;
  font-size: 32rpx;
  margin:0 auto;
  align-items: center;
  display: flex;
}
.title image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.listdiom{
  width: 86%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.times{
  padding:18rpx;
  font-size: 32rpx;
  background: #F0F3F4;
  border-radius: 20rpx;
}
.listimg{
  padding: 20rpx 0;
  width: 86%;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;

}
.imgss{
  width: 31%;
  margin: 1%;
  height: 200rpx;
  position: relative;
  overflow: hidden;
  border-radius: 14rpx;
}
.imgss image{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
}
.look{
  position: absolute;
  opacity: .6;
  background: black;
  color:white;
  width: 100%;
  padding: 6rpx 0;
  font-size: 26rpx;
  text-align: center;
  z-index: 2;
  left: 0;
  bottom: 0;
}