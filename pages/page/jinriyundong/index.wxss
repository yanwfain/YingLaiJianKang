/* pages/jinriyundong/index.wxss */
.listdelit{
  width: 90%;
  margin:0 auto;

}
.list_title{
  font-size: 34rpx;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
}

.list_cont{
  /* width: 100%; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx 20rpx;
  box-shadow: 1px 2px 20px #E0E0E0;
  border-radius: 20rpx;
  margin: 14rpx 0;
}
.left_logo{
  width: 100rpx;
  height: 100rpx;
}
.delitcont{
  width:58%;

}
.delit_title{
  font-size: 30rpx;
  padding:10rpx 0;

}
.bot_contsds{
  display: flex;
  /* justify-content: space-between; */
  align-items: center;

}
.cont_times{
  display: flex;
  align-items: center;
  padding:4rpx 10rpx;
  border-radius: 6rpx;
  background:#EEEEEE;
  
}
.cont_times image{
  width: 20rpx;
  height: 20rpx;
  margin-right: 4rpx;
}
.jifensm {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 30rpx;
}
.jifensm image{ 
  width: 28rpx;
  height: 28rpx;
}
.btnsunb{
  width: 20%;
  font-size: 28rpx;
  text-align: center;
  padding:14rpx 0;
  background-image: linear-gradient(to right, #EE7500, #F5A776);
  border-radius: 60rpx;
  color: white;
}
.btnsunbs{
  width: 20%;
  font-size: 24rpx;
  text-align: center;
  padding:14rpx 0;
  border:1px solid #EE7500;
  border-radius: 60rpx;
  color: #EE7500;
}
.submit{
  width: 90%;
  margin: 0 auto;
  padding:44rpx 0;
  display: flex;
  justify-content: center;
  align-items:center;
  font-size: 38rpx;
  border:1px dashed #BBBBBB;
  background: #F6F6F6;
  color: #767676;
  margin-top: 30rpx;
  margin-bottom: 100rpx;
  border-radius: 20rpx;
}